    <!-- Contact Section -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contact Us</h2>
                    <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                  <% if flash[:alert] %>
                    <div class="alert"><%= flash[:alert] %></div>
                  <% elsif flash[:notice] %>
                    <div class="notice"><%= flash[:notice] %></div>
                  <% end %>
                  <%= form_for @contact, :html=> {:id => 'custom_form_id'} do |f| %>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                              <%= f.text_field :name, class: 'form-control', placeholder:'Your Name *' %>
                            </div>
                            <div class="form-group">
                              <%= f.text_field :email, class: 'form-control', placeholder:'Your Email *' %>
                            </div>
                            <div class="form-group">
                              <%= f.text_field :phone, class: 'form-control', placeholder:'Your Phone *' %>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                              <%= f.text_area :message, class: 'form-control', placeholder:'Your Message *' %>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="text-center">
                          <%= f.submit 'Send', class: 'btn-xl btn'%>
                        </div>

                        <% end %>
                    </div>
                </div>
            </div>
          </div>
    </section>

